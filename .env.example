# AI Configuration Example
# Copy this to .env and set your actual API keys

# Google Gemini Configuration (Primary)
GOOGLE_GEMINI_API_KEY=your_gemini_api_key_here

# Alternative AI Providers (Optional)
# OPENAI_API_KEY=your_openai_api_key_here
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# AZURE_OPENAI_API_KEY=your_azure_api_key_here

# AI Provider Selection (defaults to google-gemini)
AI_PROVIDER=google-gemini
AI_MODEL=gemini-1.5-pro

# Performance Configuration
AI_MAX_TOKENS=8192
AI_TEMPERATURE=0.3
AI_TIMEOUT=30000

# Feature Flags (all default to false for security)
AI_ENABLE_CONTENT_ANALYSIS=false
AI_ENABLE_VISION_ANALYSIS=false
AI_ENABLE_RAG_SEARCH=false
AI_ENABLE_CHATBOT=false
AI_ENABLE_AUTO_RECOMMENDATIONS=false
AI_ENABLE_BATCH_ANALYSIS=false

# Privacy and Compliance
AI_REQUIRE_EXPLICIT_CONSENT=true
AI_LOG_ANALYTICS=false

# Rate Limiting
AI_RATE_LIMIT_RPM=60
AI_RATE_LIMIT_TPM=1000000